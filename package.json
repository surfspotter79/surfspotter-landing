    "react-dom": "^18.3.1",
    "react-leaflet": "^4.2.1",
    "stripe": "^18.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^24.3.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "gh-pages": "^6.0.0",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "rollup": "^4.46.2",
