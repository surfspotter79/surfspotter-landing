{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "gh-pages -d dist",
    "maint:on": "cp vercel.maintenance.json vercel.json && git add vercel.json && git commit -m \"chore: maintenance ON (homepage -> maintenance)\" && git push",
